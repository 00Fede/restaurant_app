<form {{action '_registrarRestaurante' on="submit"}}>
  {{input type="nombre" value=nombre placeholder="Nombre"}}
  {{input type="direccion" value=direccion placeholder="Direccion"}}
  {{input type="telefono" value=telefono placeholder="Telefono"}}
  {{input type="tipo" value=tipo placeholder="Tipo"}}
  <button type="submit">Registrar Restaurante</button>
</form>
{{outlet}}
